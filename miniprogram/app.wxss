.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
} 

button {
  background: initial;
}

button:focus{
  outline: 0;
}

button::after{
  border: none;
}
.floatIcon{
  position: fixed;
  width: 100rpx;
  height: 100rpx;
  bottom: 50rpx;
  right: 50rpx
}